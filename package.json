{
  "name": "cheatsheets",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "ts-node src/index.ts",
    "start:dev": "concurrently --kill-others \"npm run start:http\" \"npm run start:nodemon\"",
    "start:http": "http-server -c-1 dist",
    "start:nodemon": "nodemon --watch src --watch cheatsheets --ext ts,json,md,css,ejs,html -I --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/glob": "^8.1.0",
    "ejs": "^3.1.9",
    "glob": "^10.3.10",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-highlightjs": "^4.0.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/markdown-it-highlightjs": "^3.3.1",
    "@types/node": "^20.4.0",
    "concurrently": "^8.2.1",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
